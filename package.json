{
  "name": "public-api",
  "version": "0.1.0",
  "bin": {
    "public-api": "bin/public-api.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "compile": "npx esbuild ./taberogu/*.ts --platform=node --bundle --outdir=./taberogu",
    "test": "run-s compile test:run",
    "test:run": "jest",
    "deploy": "run-s compile deploy:run",
    "deploy:run": "cdk deploy"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.22.0",
    "cheerio": "^1.0.0-rc.10",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.22.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}

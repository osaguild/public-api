{
  "name": "public-api",
  "version": "0.1.0",
  "license": "MIT",
  "bin": {
    "public-api": "bin/public-api.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint --fix -c ./.eslintrc.json './**/*.ts'",
    "compile": "npx esbuild ./taberogu/*.ts --platform=node --bundle --outdir=./taberogu",
    "test": "jest",
    "deploy-dev": "run-s compile deploy:dev",
    "deploy-prd": "run-s compile deploy:prd",
    "deploy:dev": "cdk deploy --c target=dev --require-approval never",
    "deploy:prd": "cdk deploy --c target=prd --require-approval never",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "aws-cdk": "2.22.0",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.22.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
